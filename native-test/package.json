    "loki:update": "loki update",
    "loki:approve": "loki approve",
    "loki:init": "loki init",
    "prestorybook": "rnstl"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "expo": "^22.0.2",
    "react": "16.0.0-beta.5",
    "react-native": "^0.49.5"
  },
  "loki": {
    "configurations": {
      "ios.iphone7": {
        "target": "ios.simulator"
      }
    }
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": ["./storybook/stories"],
      "pattern": "**/*.js",
      "outputFile": "./storybook/storyLoader.js"
    }
